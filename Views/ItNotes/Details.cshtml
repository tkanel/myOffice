@model myITOffice.Models.ItNote

@{
    ViewData["Title"] = "Details";
}

<h4>Note details</h4>
<hr />
<div>
    <h4>ItNote</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.Label("Note description :")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Descr)
        </dd>

        <dt class="col-sm-2">
            @Html.Label("Created On :")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CreatedOn)
        </dd>
      
        <dt class="col-sm-2">
            @Html.Label("Category :")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Category.Descr)
        </dd>
        <dt class="col-sm-2">
            @Html.Label("Device type :")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DeviceType.Description)
        </dd>
        <dt class="col-sm-2">
            @Html.Label("Device name :")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Device.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.Label("User name :")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.OfficeUser.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.Label("Notes :")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Notes)
        </dd>

    </dl>
</div>
<div>
    <a class="btn btn-primary btn-sm" asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
